<core:FragmentDefinition
	xmlns:core="sap.ui.core"
	xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form"
	xmlns="sap.m">

  	<Dialog title="New Change Number">
		<f:SimpleForm
			columnsL="1"
			columnsM="1"
			labelSpanM="3"
			labelSpanL="3"
			emptySpanM="4"
			emptySpanL="4">
			<f:content>
				<Label text="Change #"/>
				<Input></Input>
				<Label text="Start Date"/>
				<DatePicker/>
			</f:content>
		</f:SimpleForm>
    	<beginButton>
      		<Button text="OK" press="onCloseCreateChangeNumberDialog"/>
    	</beginButton>
    	<endButton>
      		<Button text="Cancel" press="onCloseCreateChangeNumberDialog"/>
    	</endButton>
  	</Dialog>
</core:FragmentDefinition>